FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

  application       ABLSolver;

  libs              ("libSOWFAfiniteVolume.so" "libSOWFAincompressibleLESModels.so");

  startFrom         startTime;

  startTime         0.0;

  stopAt            endTime;
// 15
  endTime           20.0;

  deltaT            0.0025;

  writeControl      adjustableRunTime;

  writeInterval     1.0; 

  purgeWrite        0;

  writeFormat       binary;
// 12
  writePrecision    9;

  writeCompression  uncompressed;

  timeFormat        general;
// 12
  timePrecision     6;

  runTimeModifiable yes;

  adjustTimeStep    no;

  maxCo             0.75;

  maxDeltaT         25.0;

  startTimeMean     8.0;

  startTimeCorr     10.0;

